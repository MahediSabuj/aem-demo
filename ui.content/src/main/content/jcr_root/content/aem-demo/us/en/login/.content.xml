<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  jcr:primaryType="cq:Page">
  <jcr:content jcr:primaryType="cq:PageContent"
    cq:template="/conf/aem-demo/settings/wcm/templates/page-content"
    jcr:title="Login"
    sling:resourceType="aem-demo/components/page"
    hideInNav="true">
    <root jcr:primaryType="nt:unstructured"
      sling:resourceType="aem-demo/components/container"
      layout="responsiveGrid">
      <container jcr:primaryType="nt:unstructured"
        sling:resourceType="aem-demo/components/container"
        layout="responsiveGrid">
        <container jcr:primaryType="nt:unstructured"
          sling:resourceType="aem-demo/components/container"
          layout="responsiveGrid">
          <login jcr:primaryType="nt:unstructured"
            sling:resourceType="aem-demo/components/login/v1/login">
            <username jcr:primaryType="nt:unstructured"
              jcr:title="Username"
              name="username"
              required="{Boolean}true"
              type="text"
              sling:resourceType="aem-community/components/form/text"/>
            <password jcr:primaryType="nt:unstructured"
              jcr:title="Password"
              name="password"
              required="{Boolean}true"
              type="password"
              sling:resourceType="aem-community/components/form/text"/>
            <button jcr:primaryType="nt:unstructured"
              jcr:title="Login"
              name="login"
              type="submit"
              sling:resourceType="aem-community/components/form/button"/>
          </login>
        </container>
      </container>
    </root>
  </jcr:content>
</jcr:root>
